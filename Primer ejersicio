@Entity
@Table(name = "usuarios")
public class Usuario {
    
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;
    
    @NotBlank
    private String nombre;
    
    @Email
    private String correoElectronico;
    
    @NotBlank
    private String contrasenia;
    
    // constructores, getters y setters
}
